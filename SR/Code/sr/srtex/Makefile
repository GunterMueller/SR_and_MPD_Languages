##	Makefile for the SR TeX formatter, srtex.



CFLAGS = -g
SHELL = /bin/sh



OBJ = print.o lex.yy.o


srtex:$& $(OBJ)
	. ../paths.sh; $(CC) $(CFLAGS) -o srtex $(OBJ) $$LIBC

lex.yy.c: scan.l
	lex scan.l


clean:
	rm -f *.o core lex.yy.c


depend:	lex.yy.c
	sh ../depend.sh


lint:	lex.yy.c
	lint *.c >lint.raw
	sed -f ../lint.sed <lint.raw >lint.out

tags:
	/usr/local/gnuemacs/etc/etags *.c
TAGS:
	ctags *.c


################################################################################
# The rest of this file is generated by `make depend'.  Don't change it by hand.
################################################################################

# >>DO NOT DELETE THIS LINE<< #
#   "make depend" was last run:  Fri Jul 30 17:26:53 MST 1999
lex.yy.o: lex.yy.c
print.o: print.c
print.o: ../config.h
