# constants.
resource c1()
    op foo( int v)

    const int a = 5;
    const int b = 6;
    int c,d
    const int e = a+b*2;
    c = a;
    d = b;
    write(a,b,c,d,e)
    foo(b);
    foo(a);
    c++;
    d--;
    write(a,b,c,d,e)
    const int f = c+d*3
    c = 10101
    d = f-10
    write(a,b,c,d,e,f)

    proc foo(v)  {
	v++
	write("foo",v)
    }
end
